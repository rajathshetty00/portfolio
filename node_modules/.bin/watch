#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/rohan/Documents/React/Projects/portfolio/node_modules/watch/cli.js/node_modules:/home/rohan/Documents/React/Projects/portfolio/node_modules/watch/node_modules:/home/rohan/Documents/React/Projects/portfolio/node_modules:/home/rohan/Documents/React/Projects/node_modules:/home/rohan/Documents/React/node_modules:/home/rohan/Documents/node_modules:/home/rohan/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../watch/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/rohan/Documents/React/Projects/portfolio/node_modules/watch/cli.js/node_modules:/home/rohan/Documents/React/Projects/portfolio/node_modules/watch/node_modules:/home/rohan/Documents/React/Projects/portfolio/node_modules:/home/rohan/Documents/React/Projects/node_modules:/home/rohan/Documents/React/node_modules:/home/rohan/Documents/node_modules:/home/rohan/node_modules:/home/node_modules:/node_modules" node  "$basedir/../watch/cli.js" "$@"
  ret=$?
fi
exit $ret
